{
  "name": "meshage",
  "version": "0.1.13",
  "description": "A simple peer-to-peer service mesh for HTTP services",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "scripts": {
    "clean": "rimraf coverage dist tmp",
    "watch": "tsc -w -p tsconfig.release.json",
    "lint": "tslint -t stylish --type-check --exclude '**/*.js' --project 'tsconfig.json'",
    "prepublishOnly": "npm run build",
    "pretest": "npm run lint",
    "test": "jest --runInBand --coverage",
    "test:watch": "jest --watch --runInBand",
    "build": "npm run -s clean && npm run -s lint && tsc -p tsconfig.release.json",
    "build:only": "npm run -s clean && tsc -p tsconfig.release.json"
  },
  "author": "John Collier <john.collier4@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@github1/grapevine": "^1.0.6",
    "body-parser": "^1.17.2",
    "consul": "^0.33.1",
    "debug": "^3.0.1",
    "express": "^4.15.4",
    "express-http-proxy": "^1.0.6",
    "get-port": "^4.1.0",
    "hashring": "^3.2.0",
    "portfinder-sync": "0.0.2",
    "superagent": "^3.8.3",
    "superagent-debugger": "^1.2.9",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.5",
    "@types/consul": "^0.23.32",
    "@types/debug": "0.0.30",
    "@types/express": "^4.0.37",
    "@types/get-port": "4.0.1",
    "@types/node": "^8.0.26",
    "@types/superagent": "^3.8.1",
    "@types/uuid": "^3.4.3",
    "jest": "24.1.0",
    "nodemon": "^1.11.0",
    "rimraf": "^2.6.1",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.1",
    "tslint-microsoft-contrib": "^6.0.0",
    "typescript": "3.3.3"
  }
}
